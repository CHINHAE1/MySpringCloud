# 阿里云OSS配置，注意：实际使用时需要替换为真实的配置信息
aliyun:
  oss:
    endpoint: oss-cn-hangzhou.aliyuncs.com
    accessKeyId: LTAI5txxxxxxxxxxxxxxxx   # 替换为您的AccessKeyId
    accessKeySecret: xxxxxxxxxxxxxxxxxxxx  # 替换为您的AccessKeySecret
    bucketName: your-bucket-name          # 替换为您的存储桶名称
    # URL有效期(单位: 秒)
    expiration: 3600
    # 上传文件大小限制(单位: MB)
    maxSize: 100
    # 允许的文件类型
    allowedExtensions: jpg,jpeg,png,gif,bmp,doc,docx,xls,xlsx,pdf,txt,zip,rar 